@{
    ViewBag.Title = "Barang";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<script src="~/Scripts/JsCrud/crud.js"></script>
<div class="container">
    <h2>CRUD</h2>
    <form>
        <div class="form-group">
            <label for="namaBarang">Nama Barang:</label>
            <input type="text" class="form-control" id="namaBarang" placeholder="nama Barang" name="namaBarang">
        </div>
        <div class="form-group">
            <label for="qtyBarang">Jml Barang:</label>
            <input type="text" class="form-control" id="qtyBarang" placeholder="qty Barang" name="qtyBarang">
        </div>

        <button class="btn btn-default" type="submit" id="btnSimpan" onclick="fnInsert()">Simpan</button>
    </form>
    
</div>
